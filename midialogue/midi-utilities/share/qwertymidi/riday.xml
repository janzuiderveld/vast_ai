<?xml version="1.0"?>

<!--
Richard Riday's experimental isomorphic 1E/3SE keyboard, patent 5323679.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="space" action="shift-up-octave" />

<map key="backtick" action="C#3" />
<map key="1" action="D3" />
<map key="2" action="D#3" />
<map key="3" action="E3" />
<map key="4" action="F3" />
<map key="5" action="F#3" />
<map key="6" action="G3" />
<map key="7" action="G#3" />
<map key="8" action="A3" />
<map key="9" action="A#3" />
<map key="0" action="B3" />
<map key="minus" action="C4" />
<map key="equals" action="C#4" />
<map key="backspace" action="D4" />

<map key="tab" action="E3" />
<map key="Q" action="F3" />
<map key="W" action="F#3" />
<map key="E" action="G3" />
<map key="R" action="G#3" />
<map key="T" action="A3" />
<map key="Y" action="A#3" />
<map key="U" action="B3" />
<map key="I" action="C4" />
<map key="O" action="C#4" />
<map key="P" action="D4" />
<map key="open-bracket" action="D#4" />
<map key="close-bracket" action="E4" />
<map key="backslash" action="F4" />

<map key="A" action="G#3" />
<map key="S" action="A3" />
<map key="D" action="A#3" />
<map key="F" action="B3" />
<map key="G" action="C4" />
<map key="H" action="C#4" />
<map key="J" action="D4" />
<map key="K" action="D#4" />
<map key="L" action="E4" />
<map key="semicolon" action="F4" />
<map key="single-quote" action="F#4" />
<map key="enter" action="G4" />

<map key="Z" action="B3" />
<map key="X" action="C4" />
<map key="C" action="C#4" />
<map key="V" action="D4" />
<map key="B" action="D#4" />
<map key="N" action="E4" />
<map key="M" action="F4" />
<map key="comma" action="F#4" />
<map key="period" action="G4" />
<map key="slash" action="G#4" />

</qwertymidi>
